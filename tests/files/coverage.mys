func foo():
    pass

func bar(a: i64, b: i64) -> i64:
    a += 1

    if a == 2:
        b += 2
    elif a == 5:
        # Hi!
        pass

    # Ho!
    else:
        pass

    while False:
        pass

    for i in range(2):
        if i == 0:
            b += 1
        elif i == 5:
            pass

    return a + b

func fie(v: i64):
    match v:
        case 0:
            pass
        case 1:
            pass
        case _:
            bar(
                v,
                v
            )

class Foo:
    x: i64

    func foo(self) -> i64:
        return self.x

class Bar:
    x: i64

    func __init__(self):
        self.x = 9

    func foo(self) -> i64:
        return self.x

test various():
    assert bar(1, 2) == 7
    assert bar(2,
               3) == 7
    assert True
    fie(0)
    fie(5)
    assert Foo(10).foo() == 10
    assert Bar().x == 9

