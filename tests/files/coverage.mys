trait Base:

    # Pure method should not be part of code coverage.
    func foo(self):
        pass

    func bar(self):
        print(1)

    func fie(self):
        print(1)

class Fie(Base):

    func foo(self):
        pass

test trait_():
    Fie().foo()
    Fie().bar()

