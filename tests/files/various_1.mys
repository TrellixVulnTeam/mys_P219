@test
def test_2():
    assert V1 == 2
    assert V2 == 6
    assert V3[0] == 1
    assert V3[1] == 2
    assert V4 is None
    assert V5 == (1 << 2) / 2

    adder = Adder(1, 0)
    assert adder.base == 0
    assert adder.value == 1
    adder.value += 1
    assert adder.value == 2
    assert str(adder) == "Adder(value=2, base=0)"
    # Print a class should work.
    print(adder)
    assert calc(adder) == 7

@test
def test_3():
    s = "hello"
    print("s:", s, len(s), str(s))
    assert len(s) == 5
    assert str(s) == s
    assert s == "hello"
    assert s != "hello!"
    t = s
    assert s == t
    t += "!"
    assert t == "hello!"
    assert s == t
    assert str(1) == "1"
    assert str(1.0) == "1.000000"
    u = -5000
    v = str(u)
    assert v == "-5000"
    assert 4 * v == "-5000-5000-5000-5000"
    assert v * 3 == "-5000-5000-5000"
    assert v + v == "-5000-5000"
    assert fie(v) == "-5000-5000"

