V4: [bool] = None
V5: i32 = V1

def barfoo(_a: (i32, string)):
    "A docstring."

def fie(a: string) -> string:
    return 2 * a

class Adder:

    value: i32
    base: i32

    def add(self, a: i32, b: i32) -> i32:
        "Docstring."

        return self.base + self.value + a + b

def calc(adder: Adder) -> i32:
    return adder.add(1, 4)

