class AccessFoo:
    member: bool

    def func(self):
        pass

@test
def test_class_none_1():
    foo: AccessFoo = None
    assert foo is None
    print(foo.member)

@test
def test_class_none_2():
    foo: AccessFoo = None
    foo.func()

@test
def test_compare_lists_3():
    values: [bool] = None

    if values == [True]:
        pass

@test
def test_add_lists():
    l1 = [1]
    l2 = [2]

    assert l1 + l2 == [1, 2]

    l1 = None

    if l1 + l2 == [1, 2]:
        pass

@test
def test_list_acces_none():
    v: [bool] = None

    print(v[0])

@test
def test_list_len_of_none():
    v: [u8] = None

    print(len(v))

@test
def test_compare_tuples_3():
    values: (bool, i64) = None

    if values == (False, 1):
        pass

@test
def test_tuple_acces_none():
    v: (bool, i32) = None

    print(v[1])

@test
def test_tuple_unpack_in_for_loop_none_element():
    for a, b in [(True, "x"), None]:
        print(a, b)

@test
def test_string_none():
    v: string = None

    assert v is None

    v += "1"

@test
def test_string_len_of_none():
    v: string = None

    print(len(v))

@test
def test_compare_dicts_3():
    values: {bool: i64} = None

    if values == {False: 1}:
        pass

@test
def test_dict_acces_none():
    v: {bool: i32} = None

    print(v[True])

@test
def test_dict_len_of_none():
    v: {u8: string} = None

    print(len(v))

@test
def test_set_none():
    m: {string: i64} = None

    m["a"] = 1
