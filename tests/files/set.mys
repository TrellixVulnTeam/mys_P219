@test
def test_set():
    a = {1, 2, 3, 2}
    b = {2, 2, 3, 3, 4}
    assert a == {1, 2, 3}
    assert b == {2, 3, 4}

@test
def test_set_op():
    a = {1, 2, 3, 2}
    b = {2, 2, 3, 3, 4}

    assert a != b
    assert a & b == {2, 3}
    assert b - a == {4}
    assert a | b == {1, 2, 3, 4}
    assert a ^ b == {1, 4}

    s: {i32} = {1, 2, 3, 2}
    s &= {2, 3, 4}
    assert s == {2, 3}
    s |= {4}
    assert s == {2, 3, 4}
    s -= {2, 4}
    assert s == {3}
    s ^= {1, 3}
    assert s == {1}

    s = {1, 2, 3}
    assert (s & {2, 3, 4} | {5}) - {3, 5} == {2}

@test
def test_set_op_2():
    a: {i32} = {1, 2, 3}
    b: {i32} = {1, 2, 3, 4}
    c: {i32} = {1, 2}

    assert 3 in a
    assert 4 not in a
    assert 4 in b

    assert a < b
    assert c < a
    assert c < b
    assert b > c
    assert not b < c

    assert len(a) == 3
    assert len(b) == 4

    assert max(a) == 3
    assert min(a) == 1
    assert max(b) == 4
    assert min(b) == 1

@test
def test_set_type():
    a: {i32} = {3}
    assert a == {3}

    b: {i64} = {4}
    assert b == {4}

    c: {i64} = {}
    c = c | {3}
    assert c == {3}

    d: {i8} = {}
    d = d | {3}
    assert d == {3}
