def add(first: i32, second: i32) -> i32:
    """Returns the sum of given numbers `first` and `second`.

    """

    return first + second

def sub(first: i32, second: i32) -> i32:
    """Returns the diff of given numbers `first` and `second`. With a line
    break.

    """

    return first - second

