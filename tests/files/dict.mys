test dict_1():
    a = {1: 2, 3: 4}

    assert 1 in a
    assert 2 not in a
    assert a[1] == 2
    assert a[3] == 4
    assert a.get(1, -1) == 2
    assert a.get(2, -1) == -1

    a[1] = 5
    assert a[1] == 5

    try:
        print(a[4])
        res = 1
    except KeyError:
        res = 2

    assert res == 2

test dict_2():
    a: {i64: i64} = {}
    assert a.length() == 0
    a[-1] = 1
    assert a.length() == 1

test dict_3():
    a: {u8: bool} = {5: False}
    assert not a[5]
    assert a.get(4, True)

class DictFoo:
    v: i32

test dict_4():
    a: {i64: DictFoo} = {}
    assert a.length() == 0

    a[0] = DictFoo(10)
    a[1] = DictFoo(5)
    assert a[0].v == 10
    assert a[1].v == 5

    b = a[0]
    assert b.v == 10

func dict_foo(v: {i64: bool}) -> {i64: bool}:
    v[5] = True

    return v

test dict_5():
    a: {i64: bool} = {}
    b = dict_foo(a)
    assert 5 in a
    assert 5 in b
    assert a is b
    c: {i64: bool} = {}
    assert a is not c

test iterate_over_call_return_value():
    key = 0
    value = False

    for k, v in dict_foo({}):
        key = k
        value = v

    assert key == 5
    assert value

test dict_6():
    a: {u32: bool} = {1: True}

    a[1] = False
    assert not a[1]

test dict_7():
    a: {u32: u8} = {1: 5}

    assert a[1] == 5
    a[1] += 1
    assert a[1] == 6

test dict_string_value():
    a: {i64: string} = {}
    a[-1] = "erik"
    a[-1] += " m"
    assert a[-1] == "erik m"

test dict_string_key():
    a: {string: bool} = {"d": False}
    a["a"] = True
    a["b"] = False
    a["c"] = True
    assert a["a"]
    assert not a["b"]
    assert a["c"]
    assert "d" in a

test dict_bytes_key():
    a: {bytes: bool} = {b"d": False}
    a[b"a"] = True
    a[b"b"] = False
    a[b"c"] = True
    assert a[b"a"]
    assert not a[b"b"]
    assert a[b"c"]
    assert b"d" in a

test dict_bool_key():
    a: {bool: bool} = {True: False}
    a[False] = True
    assert a[False]
    assert not a[True]
    assert True in a

test dict_char_key():
    a: {char: bool} = {'d': False}
    a['a'] = True
    a['b'] = False
    a['c'] = True
    assert a['a']
    assert not a['b']
    assert a['c']
    assert 'd' in a

test dict_methods():
    d = {"a": 1, "b": 2}
    keys = d.keys()
    keys.sort()
    assert keys == ["a", "b"]

    values = d.values()
    values.sort()
    assert values == [1, 2]

    assert d.get("a", -1) == 1
    assert d.get("c", -1) == -1

    assert d.pop("a", -1) == 1
    assert d.pop("a", 5) == 5
    d.update({"z": 7, "b": 3})
    assert d.get("b", -1) == 3

    d.clear()
    assert d.length() == 0

    d.clear()
    assert d.length() == 0

test str_dict():
    assert str({1: 4}) == "{1: 4}"

test list_from_dict():
    values = list({1: "a", 2: "b"})

    assert values.length() == 2
    assert (1, "a") in values
    assert (2, "b") in values

    for pair in values:
        assert pair[0] in [1, 2]
        assert pair[1] in ["a", "b"]

    for k, v in values:
        assert k in [1, 2]
        assert v in ["a", "b"]

test iterate_over_dict_keys():
    values = {1: "a"}

    for key in values.keys():
        assert key == 1

test compare_dicts_1():
    assert {1: 2} == {1: 2}

    if {1: 2} == {1: 2}:
        pass
    else:
        assert False

    if {1: 2} != {1: 2}:
        assert False

    res = 0

    try:
        assert {1: 2} == {3: 4}
    except AssertionError:
        res = 1

    assert res == 1

test compare_dicts_2():
    res = 0

    if {1: 2} is {1: 3}:
        assert False

    try:
        assert {1: 2} is {1: 3}
    except AssertionError:
        res = 1

    if {1: 2} is not {1: 2}:
        assert False

    try:
        assert {1: 2} is not {1: 2}
    except AssertionError:
        res = 1

    assert res == 1

class StringKey:
    a: {string: i64}

test string_dict_key_in_class_member():
    assert StringKey({"1": 2}).a == {"1": 2}

test dict_str_of_none():
    v: {u8: string}? = None

    assert str(v) == "None"

test dict_in_dict():
    v: {string: {string: i64}} = {}
    assert v == {}
    assert v.length() == 0

    try:
        v["a"]["b"] = 0
    except KeyError:
        pass

    assert v.length() == 0

    v["a"] = {}
    assert v.length() == 1
    assert v["a"].length() == 0

    v["a"]["b"] = 0
    assert v.length() == 1
    assert v["a"].length() == 1
    assert v == {"a": {"b": 0}}
    assert v["a"].pop("b", -1) == 0
    assert v == {"a": {}}

test reference():
    d1 = {"1": True, "3": False}
    d2 = d1
    d2["5"] = True
    assert d1 is d2
    assert d2 == {"1": True, "3": False, "5": True}
    assert d1 == {"1": True, "3": False, "5": True}
    assert d1 == d2

test many_string_keys():
    v: {string: u8} = {}

    for i in range(1000):
        v[str(i)] = 0

    assert v.length() == 1000

test get_with_insert():
    t = {"x": ["1", "2"]}

    l = t.get("y", [], True)
    assert "y" in t
    assert l == []
    l.append("q")
    assert t.get("y") == ["q"]

    l = t.get("z", [], False)
    assert "z" not in t

class ClassAsKey:
    x: i64

test class_as_key():
    v: {ClassAsKey: bool} = {}
    v[ClassAsKey(1)] = True
    v[ClassAsKey(2)] = False
    assert v[ClassAsKey(1)]
    assert not v[ClassAsKey(2)]
    assert ClassAsKey(1) in v
    assert ClassAsKey(2) in v

test length():
    assert {1: 1, 2: 2}.length() == 2
