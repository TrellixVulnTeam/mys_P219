func char_foo(v: char) -> char:
    return v

@test
func test_char():
    a = '1'
    b = char(0x31)

    assert a == b
    assert char_foo('5') == '5'
    assert char_foo(a) == a

@test
func test_char_to_i32():
    assert i32('1') == 0x31

@test
func test_not_a_character():
    assert i32('') == 0xffff
    assert '' == '\uffff'
    assert str('') == "ï¿¿"

