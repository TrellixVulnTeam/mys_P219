func char_foo(v: char) -> char:
    return v

test char():
    a = '1'
    b = char(0x31)

    assert a == b
    assert char_foo('5') == '5'
    assert char_foo(a) == a

test char_to_i64():
    assert i64('1') == 0x31

test not_a_character():
    assert i64('') == 0xffff
    assert '' == '\uffff'
    assert str('') == "ï¿¿"

test str():
    # ToDo: Should be "'1'"
    assert str('1') == "1"

test is_alpha():
    assert not '1'.is_alpha()
    assert 'a'.is_alpha()

test is_digit():
    assert '1'.is_digit()
    assert not 'a'.is_digit()

test is_numeric():
    assert '1'.is_numeric()
    assert not 'a'.is_numeric()

test is_space():
    assert ' '.is_space()
    assert not 'a'.is_space()
