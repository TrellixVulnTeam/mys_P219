@test
def test_define_variable_in_all_branches_1():
    try:
        if False:
            x = 1
        else:
            x = 2
    except SystemExitError:
        try:
            x = 3
        except:
            if True:
                x = 4
            else:
                x = 5

    assert x == 2

def my_main(argc: i64) -> i64:
    try:
        if argc == 1:
            value = -1
        elif argc == 2:
            value = -2
        elif argc == 3:
            raise ValueError()
        else:
            raise UnreachableError()
    except ValueError:
        value = 9

    return value

@test
def test_define_variable_in_all_branches_2():
    try:
        res = 1
        assert my_main(0) == 9
    except UnreachableError:
        res = 2

    assert res == 2

    assert my_main(1) == -1
    assert my_main(2) == -2
    assert my_main(3) == 9

def define_variable_in_try_with_return_in_except() -> i64:
    try:
        x = 1
    except Error:
        return -1

    return x

@test
def test_define_variable_in_try_with_return_in_except():
    assert define_variable_in_try_with_return_in_except() == 1

def define_variable_in_try_with_raise_in_except() -> i64:
    try:
        x = 1
    except Error:
        raise

    return x

@test
def test_define_variable_in_try_with_raise_in_except():
    assert define_variable_in_try_with_raise_in_except() == 1

@test
def test_system_exit_error():
    try:
